{
  "version": 2,
  "buildCommand": "npm install",
  "outputDirectory": ".",
  "installCommand": "npm install",
  "framework": null,
  "functions": {
    "api/index.js": {
      "includeFiles": "server.js"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index"
    }
  ],
  "crons": [
    {
      "path": "/api/cleanup-unused-images",
      "schedule": "3 2 * * *"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}

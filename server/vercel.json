{
  "version": 2,
  "routes": [
    {
      "src": "/api/cleanup-unused-images",
      "dest": "/api/cleanup-unused-images.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "crons": [
    {
      "path": "/api/cleanup-unused-images",
      "schedule": "3 2 * * *"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}


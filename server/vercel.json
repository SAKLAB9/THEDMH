{
  "version": 2,
  "routes": [
    {
      "src": "/api/config",
      "dest": "/api/config.js"
    },
    {
      "src": "/api/supabase-image-url",
      "dest": "/api/supabase-image-url.js"
    },
    {
      "src": "/api/cleanup-unused-images",
      "dest": "/api/cleanup-unused-images.js"
    },
    {
      "src": "/api/upload-image",
      "dest": "/api/upload-image.js"
    },
    {
      "src": "/api/upload-board-image",
      "dest": "/api/upload-image.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "crons": [
    {
      "path": "/api/cleanup-unused-images",
      "schedule": "3 2 * * *"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}


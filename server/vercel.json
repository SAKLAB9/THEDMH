{
  "version": 2,
  "buildCommand": "npm install",
  "outputDirectory": ".",
  "installCommand": "npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/server.js"
    }
  ],
  "crons": [
    {
      "path": "/api/cleanup-unused-images",
      "schedule": "3 2 * * *"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
